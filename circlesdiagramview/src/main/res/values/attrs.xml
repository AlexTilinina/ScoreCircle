<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CirclesDiagramView">
        <attr name="circleCount" format="integer"/>
        <attr name="maxScore" format="integer"/>

        <attr name="primaryText" format="string"/>
        <attr name="innerTextColor" format="color"/>

        <attr name="secondaryText" format="string"/>
        <attr name="secondaryTextColor" format="color"/>

        <attr name="tertiaryText" format="string"/>
        <attr name="tertiaryTextColor" format="color"/>

        <attr name="circleWidth" format="dimension"/>
        <attr name="outerCircleRadius" format="dimension"/>

        <attr name="lineColor" format="color"/>
        <attr name="dotColor" format="color"/>
        <attr name="backgroundCircleColor" format="color"/>
    </declare-styleable>
</resources>